{"ast":null,"code":"import NavView from \"../NavView.vue\";\nimport HeaderComponent from \"../HeaderComponent.vue\";\nimport axios from \"axios\";\nimport LayoutDiv from \"../LayoutDiv.vue\";\nimport Swal from \"sweetalert2\";\nexport default {\n  name: \"MarketPlaceList\",\n  components: {\n    LayoutDiv\n  },\n  data() {\n    return {\n      projects: []\n    };\n  },\n  created() {\n    this.fetchProjectList();\n  },\n  methods: {\n    NavView,\n    HeaderComponent,\n    fetchProjectList() {\n      axios.get(\"/backoffice/get-features\").then(response => {\n        this.projects = response.data;\n        console.log(response.data);\n        return response;\n      }).catch(error => {\n        return error;\n      });\n    },\n    handleDelete(id) {\n      Swal.fire({\n        title: \"Are you sure?\",\n        text: \"You won't be able to revert this!\",\n        icon: \"warning\",\n        showCancelButton: true,\n        confirmButtonColor: \"#3085d6\",\n        cancelButtonColor: \"#d33\",\n        confirmButtonText: \"Yes, delete it!\"\n      }).then(result => {\n        if (result.isConfirmed) {\n          axios.delete(`/backoffice/delete-feature${id}`).then(response => {\n            Swal.fire({\n              icon: \"success\",\n              title: \"Project deleted successfully!\",\n              showConfirmButton: false,\n              timer: 1500\n            });\n            this.fetchProjectList();\n            return response;\n          }).catch(error => {\n            Swal.fire({\n              icon: \"error\",\n              title: \"An Error Occured!\",\n              showConfirmButton: false,\n              timer: 1500\n            });\n            return error;\n          });\n        }\n      });\n    }\n  }\n};","map":{"version":3,"names":["NavView","HeaderComponent","axios","LayoutDiv","Swal","name","components","data","projects","created","fetchProjectList","methods","get","then","response","console","log","catch","error","handleDelete","id","fire","title","text","icon","showCancelButton","confirmButtonColor","cancelButtonColor","confirmButtonText","result","isConfirmed","delete","showConfirmButton","timer"],"sources":["/Users/brunocastro/CUSTOM/CLIENTES/casinoGate/casinogate_backoffice/src/components/MarketPlace/MarketPlaceList.vue"],"sourcesContent":["<template>\n  <NavView />\n  <div id=\"MainView\">\n    <HeaderComponent :breadcrumbs=\"breadcrumbs\" />\n    <div class=\"content-block\">\n      <layout-div>\n        <div class=\"container\">\n          <div class=\"card\">\n            <div class=\"card-header\">\n              <router-link to=\"/create\" class=\"btn btn-outline-primary\"\n                >Create New Project\n              </router-link>\n            </div>\n            <div class=\"card-body\">\n              <table class=\"table table-bordered\">\n                <thead>\n                  <tr>\n                    <th>Name</th>\n                    <th>Description</th>\n                    <th width=\"240px\">Action</th>\n                  </tr>\n                </thead>\n                <tbody>\n                  <tr v-for=\"project in projects\" :key=\"project.id\">\n                    <td>{{ project.name }}</td>\n                    <td>{{ project.description }}</td>\n                    <td>\n                      <router-link\n                        :to=\"`/show/${project.id}`\"\n                        class=\"btn btn-outline-info mx-1\"\n                        >Show</router-link\n                      >\n                      <router-link\n                        :to=\"`/edit/${project.id}`\"\n                        class=\"btn btn-outline-success mx-1\"\n                        >Edit</router-link\n                      >\n                      <button\n                        @click=\"handleDelete(project.id)\"\n                        className=\"btn btn-outline-danger mx-1\"\n                      >\n                        Delete\n                      </button>\n                    </td>\n                  </tr>\n                </tbody>\n              </table>\n            </div>\n          </div>\n        </div>\n      </layout-div>\n    </div>\n  </div>\n</template>\n\n<script>\nimport NavView from \"../NavView.vue\";\nimport HeaderComponent from \"../HeaderComponent.vue\";\nimport axios from \"axios\";\nimport LayoutDiv from \"../LayoutDiv.vue\";\nimport Swal from \"sweetalert2\";\n\nexport default {\n  name: \"MarketPlaceList\",\n  components: {\n    LayoutDiv,\n  },\n  data() {\n    return {\n      projects: [],\n    };\n  },\n  created() {\n    this.fetchProjectList();\n  },\n  methods: {\n    NavView,\n    HeaderComponent,\n    fetchProjectList() {\n      axios\n        .get(\"/backoffice/get-features\")\n        .then((response) => {\n          this.projects = response.data;\n          console.log(response.data);\n          return response;\n        })\n        .catch((error) => {\n          return error;\n        });\n    },\n    handleDelete(id) {\n      Swal.fire({\n        title: \"Are you sure?\",\n        text: \"You won't be able to revert this!\",\n        icon: \"warning\",\n        showCancelButton: true,\n        confirmButtonColor: \"#3085d6\",\n        cancelButtonColor: \"#d33\",\n        confirmButtonText: \"Yes, delete it!\",\n      }).then((result) => {\n        if (result.isConfirmed) {\n          axios\n            .delete(`/backoffice/delete-feature${id}`)\n            .then((response) => {\n              Swal.fire({\n                icon: \"success\",\n                title: \"Project deleted successfully!\",\n                showConfirmButton: false,\n                timer: 1500,\n              });\n              this.fetchProjectList();\n              return response;\n            })\n            .catch((error) => {\n              Swal.fire({\n                icon: \"error\",\n                title: \"An Error Occured!\",\n                showConfirmButton: false,\n                timer: 1500,\n              });\n              return error;\n            });\n        }\n      });\n    },\n  },\n};\n</script>\n"],"mappings":"AAwDA,OAAOA,OAAM,MAAO,gBAAgB;AACpC,OAAOC,eAAc,MAAO,wBAAwB;AACpD,OAAOC,KAAI,MAAO,OAAO;AACzB,OAAOC,SAAQ,MAAO,kBAAkB;AACxC,OAAOC,IAAG,MAAO,aAAa;AAE9B,eAAe;EACbC,IAAI,EAAE,iBAAiB;EACvBC,UAAU,EAAE;IACVH;EACF,CAAC;EACDI,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,QAAQ,EAAE;IACZ,CAAC;EACH,CAAC;EACDC,OAAOA,CAAA,EAAG;IACR,IAAI,CAACC,gBAAgB,CAAC,CAAC;EACzB,CAAC;EACDC,OAAO,EAAE;IACPX,OAAO;IACPC,eAAe;IACfS,gBAAgBA,CAAA,EAAG;MACjBR,KAAI,CACDU,GAAG,CAAC,0BAA0B,EAC9BC,IAAI,CAAEC,QAAQ,IAAK;QAClB,IAAI,CAACN,QAAO,GAAIM,QAAQ,CAACP,IAAI;QAC7BQ,OAAO,CAACC,GAAG,CAACF,QAAQ,CAACP,IAAI,CAAC;QAC1B,OAAOO,QAAQ;MACjB,CAAC,EACAG,KAAK,CAAEC,KAAK,IAAK;QAChB,OAAOA,KAAK;MACd,CAAC,CAAC;IACN,CAAC;IACDC,YAAYA,CAACC,EAAE,EAAE;MACfhB,IAAI,CAACiB,IAAI,CAAC;QACRC,KAAK,EAAE,eAAe;QACtBC,IAAI,EAAE,mCAAmC;QACzCC,IAAI,EAAE,SAAS;QACfC,gBAAgB,EAAE,IAAI;QACtBC,kBAAkB,EAAE,SAAS;QAC7BC,iBAAiB,EAAE,MAAM;QACzBC,iBAAiB,EAAE;MACrB,CAAC,CAAC,CAACf,IAAI,CAAEgB,MAAM,IAAK;QAClB,IAAIA,MAAM,CAACC,WAAW,EAAE;UACtB5B,KAAI,CACD6B,MAAM,CAAE,6BAA4BX,EAAG,EAAC,EACxCP,IAAI,CAAEC,QAAQ,IAAK;YAClBV,IAAI,CAACiB,IAAI,CAAC;cACRG,IAAI,EAAE,SAAS;cACfF,KAAK,EAAE,+BAA+B;cACtCU,iBAAiB,EAAE,KAAK;cACxBC,KAAK,EAAE;YACT,CAAC,CAAC;YACF,IAAI,CAACvB,gBAAgB,CAAC,CAAC;YACvB,OAAOI,QAAQ;UACjB,CAAC,EACAG,KAAK,CAAEC,KAAK,IAAK;YAChBd,IAAI,CAACiB,IAAI,CAAC;cACRG,IAAI,EAAE,OAAO;cACbF,KAAK,EAAE,mBAAmB;cAC1BU,iBAAiB,EAAE,KAAK;cACxBC,KAAK,EAAE;YACT,CAAC,CAAC;YACF,OAAOf,KAAK;UACd,CAAC,CAAC;QACN;MACF,CAAC,CAAC;IACJ;EACF;AACF,CAAC","ignoreList":[]},"metadata":{},"sourceType":"module","externalDependencies":[]}